using System.Windows.Forms;

namespace Appnumber
{
    public partial class Form1 : Form
    {
        public Form1()
        {
            InitializeComponent();
        }

        private void textBox1_TextChanged(object sender, EventArgs e)
        {

        }

        private void label3_Click(object sender, EventArgs e)
        {

        }

        private void button_click(object sender, EventArgs e)
        {
                
        }

        private void butt_clr(object sender, EventArgs e)
        {
            if (output.TextLength > 0)
            {
                output.Text = output.Text.Remove(output.TextLength - 1);
            }

        }

        private void butto_clean(object sender, EventArgs e)
        {
            output.Clear();
        }

        private void butt_start(object sender, EventArgs e)
        {
            int val = int.Parse(output.Text);
            for(int i = 1; i <= val; i++)
            {
                richTextBox1.Text += $"{i}\n";
                
                if(i % 2 == 0)
                {
                    richTextBox3.Text += $"{i}\n";
                }
                else
                {
                    richTextBox2.Text += $"{i}\n";
                }
                
            }
            for (int x = 2; x <= val; x++)
            {
                bool tub = true;
                for (int j = 2; j < Math.Sqrt(x); j++)
                {
                    if(x % j == 0)
                        {
                            tub = false;
                            break;
                        }
                }
                if (tub == true)
                    richTextBox4.Text += $"{x}\n";

            }
            
        }

        private void button1_KeyDown(object sender, KeyEventArgs e)
        {

        }

        private void label5_Click(object sender, EventArgs e)
        {

        }

        private void button4_Click(object sender, EventArgs e)
        {
            richTextBox1.Clear();
            richTextBox2.Clear();
            richTextBox3.Clear();
            richTextBox4.Clear();

        }
    }
}
